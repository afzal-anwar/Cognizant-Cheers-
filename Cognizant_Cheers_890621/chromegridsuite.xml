<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ChromeGridSuite">
	<parameter name="browserCodes" value="4" />
	<listeners>
		<listener
			class-name="findinghospitals.utilities.Transformation"></listener>
	</listeners>
	<test name="SmokeTest">
		<groups>
			<run>
				<include name="Smoke" />
				<include name="Functional" />
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="cheers.cognizant.testcases.TestCases" />
		</classes>
	</test>
</suite>
